<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <system.serviceModel>
        
        <services>
            <service name="ProductLicenseServcie.Services.TokenLicenseService">
              <host>
                <baseAddresses>
                  <add baseAddress="http://localhost:8080"/>
                </baseAddresses>
              </host>
                <endpoint address="net.pipe://localhost/TokenLicSrv" 
                          binding="netNamedPipeBinding"
                          contract="ProductLicenseServcie.Services.ITokenLicenseService" />
            </service>
        </services>
      <behaviors>
        <serviceBehaviors>
          <behavior>
            <serviceMetadata httpGetEnabled="true"/>
          </behavior>
        </serviceBehaviors>
      </behaviors>
    </system.serviceModel>
</configuration>